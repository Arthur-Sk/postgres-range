{
    "name": "belamov/postrges-range",
    "description": "Postgres range types support for laravel",
    "keywords": [
        "belamov",
        "postrges-range",
        "laravel",
        "postgres"
    ],
    "homepage": "https://github.com/belamov/postrges-range",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "belamov",
            "email": "business@belamov.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^7.0",
        "nesbot/carbon": "^2.16"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Belamov\\PostrgesRange\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Belamov\\PostrgesRange\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Belamov\\PostrgesRange\\PostrgesRangeServiceProvider"
            ]
        }
    }
}
